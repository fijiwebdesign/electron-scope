{
  "name": "electron-scope",
  "productName": "Electron Scope",
  "version": "0.0.1",
  "description": "Node JS Debugger using Electron",
  "license": "MIT",
  "repository": "fijiwebdesign/electron-scope",
  "author": {
    "name": "Gabe",
    "email": "gabe@fijiwebdesign.com",
    "url": "https://github.com/fijiwebdesign/electron-scope"
  },
  "bin": {
    "electron-scope": "bin/electron-scope.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build": "electron-packager . --out=dist --app-version=$npm_package_version --prune --asar --overwrite --all"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron",
    "debugger",
    "node",
    "inspector"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "electron-debug": "^1.0.0",
    "electron-reload": "^1.5.0"
  },
  "devDependencies": {
    "devtron": "^1.1.0",
    "electron": "^18.3.7",
    "electron-packager": "^14.0.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
